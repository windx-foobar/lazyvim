{
  "$schema": "https://www.schemastore.org/prettierrc.json",
  "singleQuote": true,
  "tabWidth": 2,
  "arrowParens": "always",
  "bracketSameLine": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "semi": true,
  "printWidth": 120,
  "overrides": [
    {
      "files": ["*.scss", "*.css"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
